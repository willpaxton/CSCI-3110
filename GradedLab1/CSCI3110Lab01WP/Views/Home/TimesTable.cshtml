@{
    ViewData["Title"] = "Times Table";

    int[,] timesTable = new int[12, 12];
    
    @for(int i = 1; i <= 12; i++){
        @for(int j = 1; j <= 12; j++){
            timesTable[i-1, j-1] = i * j;
        }
    }
}

<h1>Times Table</h1>

<table class="table table-bordered w-25">
    @for(int i = 1; i <= 12; i++){
        <tr>
            @for(int j = 1; j <= 12; j++){
                <td>@timesTable[i-1, j-1]</td>
            }
        </tr>
    }
</table>